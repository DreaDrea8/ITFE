{
  "name": "ITFE",
  "version": "0.0.0",
  "patch": "0",
  "description": "ITFE - backend",
  "main": "index.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc & resolve-tspaths",
    "start:local": "cross-env NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register --env-file=../.env --env-file=../.env.local dist/index.js",
    "start:local:prod": "cross-env NODE_ENV=production tsc & resolve-tspaths & cross-env NODE_ENV=production node --env-file=../.env --env-file=../.env.local dist/index.js",
    "start:local:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --env-file=../.env --env-file=../.env.local src/index.ts",
    "start": "cross-env NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register --env-file=.env dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --poll -r tsconfig-paths/register --respawn --transpile-only --env-file=.env src/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22.8.2",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "globals": "^15.8.0",
    "mysql2": "^3.11.3",
    "resolve-tspaths": "^0.8.19",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
